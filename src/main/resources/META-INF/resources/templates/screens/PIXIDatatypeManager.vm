#* @vtlvariable name="animalModels" type="java.util.List<org.nrg.xnatx.plugins.pixi.models.AnimalModel>" *#
#* @vtlvariable name="pdxs" type="java.util.List<org.nrg.xnatx.plugins.pixi.entities.PDXEntity>" *#
<table class="xnat-table" style="margin: 15px 0 15px 0">
    <tr>
        <th>Animal Model Name</th>
        <th>Animal Model ID</th>
        <th>Actions</th>
    </tr>
    #foreach($animalModel in $animalModels)
        <tr>
            <td>$animalModel.getName()</td>
            <td>$animalModel.getId()</td>
            <td><a href="$link.setPage("XHBMScreen_report_pixi_animalModel.vm").addQueryData("id", $animalModel.getId())"><button class="btn sm">View</button></a>
                <a href="$link.setPage("XHBMScreen_edit_pixi_animalModel.vm").addQueryData("id", $animalModel.getId())"><button class="btn sm">Edit</button></a></td>
        </tr>
    #end
</table>

<a href="$link.setPage("XHBMScreen_edit_pixi_animalModel.vm")"><button class="btn sm">New Animal Model</button></a>

<table class="xnat-table" style="margin: 15px 0 15px 0">
    <tr>
        <th>PDX</th>
        <th>PDX ID</th>
        <th>Actions</th>
    </tr>
    #foreach($pdx in $pdxs)
        <tr>
            <td>$pdx.getPdxLabel()</td>
            <td>$pdx.getPdxID()</td>
            <td><a href="$link.setPage("XHBMScreen_report_pixi_pdx.vm").addQueryData("id", $pdx.getPdxID())"><button class="btn sm">View</button></a>
                <a href="$link.setPage("XHBMScreen_edit_pixi_pdx.vm").addQueryData("id", $pdx.getPdxID())"><button class="btn sm">Edit</button></a></td>
        </tr>
    #end
</table>

<a href="$link.setPage("XHBMScreen_edit_pixi_pdx.vm")"><button class="btn sm">New PDX</button></a>